{
    "name": "vendor/laravel-package",
    "description": "Laravel package boilerplate",
    "type": "library",
    "license": "MIT",

    "minimum-stability": "dev",
    "prefer-stable": true,

    "require": {
        "php": "^8.0"
    },

    "require-dev": {
        "orchestra/testbench": "^7.21",
        "phpunit/phpunit": "^9.5.10",
        "brianium/paratest": "^6.0",
        "phpstan/phpstan": "^1.9",
        "nunomaduro/larastan": "^2.4"
    },

    "autoload": {
        "psr-4": {
            "Vendor\\LaravelPackage\\": "src"
        }
    },
    
    "autoload-dev": {
        "psr-4": {
            "Vendor\\LaravelPackage\\Tests\\": "tests"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\LaravelPackage\\PackageServiceProvider"
            ]
        }
    }
}
